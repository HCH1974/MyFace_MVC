<html lang="en-gb">
<%- include('_head') -%>
    <%- include('_header') -%>

        <body>
            <h1>Create User</h1>
            <form method="post" action="/users/create">
                <label>
                    Name:
                    <input type="text" required name="name" />
                </label>
                <label>
                    Username:
                    <input type="text" required name="username" />
                </label>
                <label>
                    Email:
                    <input type="email" required name="email" />
                </label>
                <label>
                    ProfileImageUrl:
                    <input type="text" required name="profileImageUrl" />
                </label>
                <label>
                    CoverImageUrl:
                    <input type="text" required name="coverImageUrl" />
                </label>
                <button type="submit">Submit</button>
            </form>
        </body>
        <%- include('_footer') -%>

</html>