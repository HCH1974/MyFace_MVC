<html lang="en-gb"> <%- include('_head') -%> <%- include('_header') -%>

        <body>
            <h1 class="page-title">Users</h1>
            <ul>
                <% results.forEach(function(user) {%>
                    <li class="list-of-users">
                        <div class="individual-user">
                            <%= user.name %>
                        </div> <a class="user-button" href="/users/<%= user.id %>">Details</a>
                    </li> <br>
                    <% }); %>
            </ul>
            <%if (next) { %> <a href="<%= next %>">Next</a>
                <% } %>
                    <%if (previous) { %> <a href="<%= previous %>">Previous</a>
                        <% } %>
        </body> <%- include('_footer') -%>
=</html>